  let points = [];
  for (let i = 0; i < 20; i++) {
    if (i % 2 === 0) {
      points.push({
        latitude: -23.5489 + i * 0.001,
        longitude: -46.6388 + i * 0.001
      });
    } else {
      points.push({
        latitude: -23.5489 - i * 0.002,
        longitude: -46.6388 + i * 0.001
      });
    }
  }